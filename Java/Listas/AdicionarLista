
package javaapplication1;
import java.util.ArrayList;
import java.util.Collections;
import java.util.Random;
import java.util.Set;
import java.util.TreeSet;

public class JavaApplication1 {

    public static void main(String[] args) {
        ArrayList <Integer> lista = new ArrayList <Integer>();
        Random r = new Random();
        
        for (int i = 0; i < 20; i++) {
            lista.add(r.nextInt());
        }
        
        System.out.println(lista);
        Collections.sort(lista);
        System.out.println(lista);
        
        ArrayList <String> familiaE = new ArrayList <String>();
        familiaE.add("Lineuzinho");
        familiaE.add("Dona Nene");
        familiaE.add("Tuco");
        
        ArrayList <String> familiaA = new ArrayList <String>();
        familiaA.add("Augustinho Carrara");
        familiaA.add("Doria");
        familiaA.add("Marquinhos");
        
        ArrayList <String> familia = new ArrayList <String>();

        familia.addAll(familiaE);
        familia.addAll(familiaA);
        
        Collections.sort(familia);
        System.out.print(familia);
        
    }
    
}
